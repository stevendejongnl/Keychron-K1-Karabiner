{
  "title": "Keychron K1 : Replace Screenshot button with new screenshot",
  "rules": [
    {
      "description": "Keychron K1 : Change Screenshot (left_gui+left_shift+4) to new screenshot.",
      "manipulators": [
        {
          "from": {
            "simultaneous": [
              {
                "key_code": "left_gui"
              },
              {
                "key_code": "left_shift"
              },
              {
                "key_code": "4"
              }
            ]
          },
          "to": [
            {
              "key_code": "left_gui"
            },
            {
              "key_code": "left_shift"
            },
            {
              "key_code": "5"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
